    function getNum(action) {
        currNum = parseInt($(".pro-bigimage img").get(0).name);
        $(".pro-thumb img").each(function () {
            $(this).removeClass("current");
        })

        if (action == "prev") {
            if (currNum == 0) {
                currNum = maxNum

            };
            $(".pro-thumb img").get(currNum - 1).className = "current";
            $(".pro-bigimage img").hide();
            $(".pro-bigimage img").get(0).src = imgUrl + $(".pro-thumb img").get(currNum - 1).name.split("||")[1];
            $(".pro-bigimage img").get(0).name = $(".pro-thumb img").get(currNum - 1).name.split("||")[0];
            $(".pro-bigimage img").fadeIn();
        } else {
            if (currNum == maxNum - 1) {
                currNum = -1

            };
            $(".pro-thumb img").get(currNum + 1).className = "current";
            $(".pro-bigimage img").hide();
            $(".pro-bigimage img").get(0).src = imgUrl + $(".pro-thumb img").get(currNum + 1).name.split("||")[1];
            $(".pro-bigimage img").get(0).name = $(".pro-thumb img").get(currNum + 1).name.split("||")[0];
            $(".pro-bigimage img").fadeIn();
        }
    }

